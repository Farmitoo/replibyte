{
    "name": "farmitoo/replibyte",
    "type": "project",
    "description": "Replibyte - Seed staging & development databases with a subset of data from another database",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/yaml": "^6.1",
        "symfony/console": "^6.1",
        "ext-pdo": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.13",
        "symfony/dotenv": "^6.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fakerphp/faker": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.1",
        "friendsofphp/php-cs-fixer": "^3.13",
        "gd-75/php-cs-fixer-enforce-double-quotes": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Farmitoo\\ReplibyteBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farmitoo\\ReplibyteBundle\\Tests\\": "tests"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
